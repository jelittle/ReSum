{
  "name": "Chrome Extension",
  "description": "review summarizer",
  "version": "1.0.0",
  "manifest_version": 3,
  "author": "Joshua Little",
  "icons": {
    "16" : "favicon.ico",
    "48" : "favicon.ico",
    "128" : "favicon.ico"
  },
  "action": {
    "default_popup":"index.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://www.yelp.com/biz/*"
      ]
    }
  ]
}